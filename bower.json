{
  "name": "bitaculous-assetify",
  "description": "Assets used by Bitaculous, packaged for Bower.",
  "keywords": ["Assetify", "Bitaculous"],
  "homepage": "https://bitaculous.github.io/assetify/",
  "authors": [
    "Maik Kempe <mkempe@bitaculous.com> (https://bitaculous.com)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/bitaculous/assetify.git"
  },
  "ignore": [
    "**/.*",
    "*.gemspec",
    "*.sublime-project",
    "Gemfile",
    "Guardfile",
    "lib",
    "package.json",
    "Rakefile",
    "resources",
    "spec",
    "tasks"
  ],
  "main": [
    "assets/stylesheets/bitaculous/assetify.sass"
  ],
  "dependencies": {
    "bourbon": "~6.0.0",
    "breakpoint-sass": "~2.7.1",
    "modularscale-sass": "~3.0.8",
    "sass-mediaqueries": "~1.6.1",
    "sass-rem": "~2.0.1",
    "sassy-maps": "at-import/Sassy-Maps"
  },
  "devDependencies": {
    "modernizr": "~3.7.1",
    "normalize.css": "~8.0.1"
  }
}
